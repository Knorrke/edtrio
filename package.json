{
    "private": true,
    "name": "edtrio",
    "version": "0.1.0",
    "main": "dist/index.js",
    "homepage": "http://schul-cloud.github.io/edtrio",
    "scripts": {
        "docz:dev": "docz dev",
        "docz:build": "docz build",
        "dev": "parcel src/index.html -d dist/dev --open",
        "build": "rm -rf dist && parcel build src/index.js"
    },
    "dependencies": {
        "react": "^16.8.1",
        "react-dom": "^16.8.1",
        "react-router": "^4.3.1",
        "react-router-dom": "^4.3.1",
        "styled-components": "^4.1.3"
    },
    "devDependencies": {
        "@babel/core": "^7.2.2",
        "@babel/preset-env": "^7.3.1",
        "@babel/preset-react": "^7.0.0",
        "babel-plugin-module-resolver": "^3.1.3",
        "babel-plugin-styled-components": "^1.10.0",
        "docz": "^0.11.2",
        "docz-theme-default": "^0.11.2",
        "eslint": "^5.13.0",
        "eslint-config-prettier": "^3.6.0",
        "eslint-config-standard": "^12.0.0",
        "eslint-plugin-babel": "^5.2.1",
        "eslint-plugin-prettier": "^3.0.1",
        "eslint-plugin-react": "^7.12.4",
        "eslint-plugin-styled-components": "0.0.0",
        "eslint-plugin-styled-components-config": "0.0.2",
        "gh-pages": "^2.0.1",
        "husky": "^1.0.0-rc.15",
        "lint-staged": "^7.3.0",
        "parcel-bundler": "^1.11.0",
        "prettier": "^1.14.3",
        "react-hot-loader": "^4.6.5",
        "typescript": "^3.2.1"
    },
    "lint-staged": {
        "linters": {
            "edtrio-client/src/**/*.{js,jsx}": [
                "./node_modules/.bin/eslint --ext .jsx --ext .js ./",
                "prettier",
                "git add"
            ],
            "edtrio-client/src/**/*.{ts,tsx}": [
                "./node_modules/.bin/tslint -c tslint.json --project edtrio-client/tsconfig.json",
                "prettier",
                "git add"
            ],
            "edtrio-server/src/**/*.{js,jsx}": [
                "./node_modules/.bin/eslint --ext .jsx --ext .js ./",
                "prettier",
                "git add"
            ],
            "edtrio-server/src/**/*{ts,tsx}": [
                "./node_modules/.bin/tslint -c tslint.json --project edtrio-server/tsconfig.json",
                "prettier",
                "git add"
            ]
        },
        "ignore": [
            "**/generated/**/*.{ts,tsx}"
        ]
    },
    "browserslist": [
        ">0.2%",
        "not dead",
        "not ie <= 11",
        "not op_mini all"
    ],
    "author": {
        "name": "Florian Wirtz",
        "email": "florian.wirtz@hpi-alumni.de",
        "url": "https://github.com/FWirtz"
    },
    "contributors": [
        {
