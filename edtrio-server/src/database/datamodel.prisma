type User {
  id: ID! @unique
  name: String!
  isTeacher: Boolean!
  createdAt: DateTime!
  updatedAt: DateTime!
}

type Document {
  id: ID! @unique
  value: String!
  users: [User!]!
  createdAt: DateTime!
  updatedAt: DateTime!
}

type MultipleChoiceAnswer {
  id: ID! @unique
  isCorrect: Boolean!
  createdAt: DateTime!
  updatedAt: DateTime!
}
